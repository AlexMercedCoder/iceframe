# Iceberg Catalog Configuration
# Copy this file to .env and fill in your credentials

# REST Catalog URI
ICEBERG_CATALOG_URI=https://catalog.dremio.cloud/api/iceberg

# OAuth2 Server URI (for credential vending)
ICEBERG_OAUTH2_SERVER_URI=https://login.dremio.cloud/oauth/token

# Access Token
ICEBERG_TOKEN=your_token_here

# Warehouse/Project Name
ICEBERG_WAREHOUSE=your_warehouse_name

# Catalog Type
ICEBERG_CATALOG_TYPE=rest

# Optional: Enable credential vending
ICEBERG_CREDENTIAL_VENDING=vended-credentials
